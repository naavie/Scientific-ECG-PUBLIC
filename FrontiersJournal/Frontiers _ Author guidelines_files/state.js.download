,content:[{nodeType:a,value:fP,marks:[],data:{}}]},{nodeType:F,data:{},content:[{nodeType:k,data:{},content:[{nodeType:f,data:{},content:[{nodeType:a,value:"the corresponding author must be affiliated with the Leibniz Institute for Zoo and Wildlife Research.\r",marks:[],data:{}}]}]},{nodeType:k,data:{},content:[{nodeType:f,data:{},content:[{nodeType:a,value:"the corresponding author uses their Leibniz Institute for Zoo and Wildlife Research email address.",marks:[],data:{}}]}]}]},{nodeType:B,data:{},content:[{nodeType:a,value:dG,marks:[],data:{}}]},{nodeType:f,data:{},content:[{nodeType:a,value:cc,marks:[],data:{}},{nodeType:v,data:{uri:bs},content:[{nodeType:a,value:bJ,marks:[],data:{}}]},{nodeType:a,value:L,marks:[],data:{}}]},{nodeType:f,data:{},content:[{nodeType:a,value:cd,marks:[],data:{}},{nodeType:v,data:{uri:ba},content:[{nodeType:a,value:bA,marks:[],data:{}}]},{nodeType:a,value:L,marks:[],data:{}}]}]}},links:{type:c,generated:e,id:"$ROOT_QUERY.institutionalMemberCollection({\"limit\":1,\"where\":{\"slug\":\"leibniz-institute-for-zoo-and-wildlife-research-izw\"}}).items.0.main.links",typename:an},__typename:aj}},"$ROOT_QUERY.institutionalMemberCollection({\"limit\":1,\"where\":{\"slug\":\"leibniz-institute-for-zoo-and-wildlife-research-izw\"}}).items.0.main.links.assets":{writable:true,enumerable:true,value:{__typename:ao,block:[]}},"$ROOT_QUERY.institutionalMemberCollection({\"limit\":1,\"where\":{\"slug\":\"leibniz-institute-for-zoo-and-wildlife-research-izw\"}}).items.0.main.links":{writable:true,enumerable:true,value:{assets:{type:c,generated:e,id:"$ROOT_QUERY.institutionalMemberCollection({\"limit\":1,\"where\":{\"slug\":\"leibniz-institute-for-zoo-and-wildlife-research-izw\"}}).items.0.main.links.assets",typename:ao},entries:{type:c,generated:e,id:"$ROOT_QUERY.institutionalMemberCollection({\"limit\":1,\"where\":{\"slug\":\"leibniz-institute-for-zoo-and-wildlife-research-izw\"}}).items.0.main.links.entries",typename:ap},__typename:an}},"$ROOT_QUERY.institutionalMemberCollection({\"limit\":1,\"where\":{\"slug\":\"leibniz-institute-for-zoo-and-wildlife-research-izw\"}}).items.0.main.links.entries":{writable:true,enumerable:true,value:{inline:[],block:[],__typename:ap}},"$ROOT_QUERY.basicContentPage({\"id\":\"3FJUcPYns4wtso1JmYkXvH\",\"preview\":false})":{writable:true,enumerable:true,value:{navigationMenu:b,content:{type:c,generated:e,id:"$ROOT_QUERY.basicContentPage({\"id\":\"3FJUcPYns4wtso1JmYkXvH\",\"preview\":false}).content",typename:ew},__typename:ev}},"$ROOT_QUERY.basicContentPage({\"id\":\"3FJUcPYns4wtso1JmYkXvH\",\"preview\":false}).content":{writable:true,enumerable:true,value:{json:{type:g,json:{data:{},content:[{data:{},content:[{data:{},marks:[],value:nv,nodeType:a}],nodeType:aB},{data:{},content:[{data:{},marks:[],value:"Frontiers' ",nodeType:a},{data:{uri:iu},content:[{data:{},marks:[],value:ajz,nodeType:a}],nodeType:v},{data:{},marks:[],value:" is unique and quality-focused. Read on for a guide for reviewers on what to expect of our peer review process.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"What is Frontiers' peer review model?",nodeType:a}],nodeType:B},{data:{},content:[{data:{},marks:[],value:"Frontiers operates a ",nodeType:a},{data:{},marks:[],value:Dx,nodeType:a},{data:{},marks:[],value:" model during the review process. This means the reviewers know who the authors are in order to offer a full assessment within the context of their research and to ensure they can avoid any potential competing interests in accepting a review invitation.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"Frontiers also believes that reviewers should be acknowledged for their work in conducting peer review. We believe in transparency and ensuring no bias during the peer review process. This is why we disclose the name of all endorsing reviewers upon publication, for every article we publish.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"What to expect from our collaborative peer review",nodeType:a}],nodeType:B},{data:{},content:[{data:{},marks:[],value:"Our peer review platform is collaborative: it unites authors, reviewers, and the handling editor in a direct online dialog, enabling quick iterations and facilitating consensus. Editors and reviewers work with the authors to improve their manuscript.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:ajA,nodeType:a}],nodeType:U},{data:{},content:[{data:{},marks:[],value:"Once a reviewer accepts the invitation to review, they are sent an email with a link to the online review forum. In the review forum, they can access and review the manuscript and supporting documents. During this first review phase the reviewers assess the paper independently from each other and the authors.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"The review is completed by answering a review questionnaire provided in the review forum, and tailored for each article type (original research, review, study protocol, clinical trial, etc). When submitting their review report, a reviewer will also submit their recommendation to the editor. At this stage they are able to directly endorse the manuscript and finalize their review process, should the manuscript meet our acceptance criteria.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"Once all reviewers have submitted their review report, the handling editor is responsible for activating the next phase of the process: the interactive review. Even if the review reports are unfavorable to the authors, the collaborative review forum is activated to allow authors the opportunity of a rebuttal.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:ajB,nodeType:a}],nodeType:U},{data:{},content:[{data:{},marks:[],value:"Once the interactive review phase is activated, authors are notified and are able to view and respond to reviewers' comments within the review forum. Reviewers are notified when the author has replied in full, and\u002For resubmitted their manuscript in line with reviewer comments.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"If needed, reviewers can enter a dialog with the author to request clarifications or further revisions. They can also access and comment on other reviewers' review reports.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"If the reviewer feels the authors have made the required changes and the paper is suitable for publication, they may endorse it. Alternatively, if it is felt that the authors have not or cannot bring the paper up to standard, reviewers can recommend that it be rejected.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"How to peer review",nodeType:a}],nodeType:B},{data:{},content:[{data:{},marks:[],value:"Reviewers are the crucial facilitator between the author and the handling editor. From a position of expertise, reviewers guide and enable fellow researchers to get their work out into the world, in the best condition it can be.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"To support our reviewers, we have put together some tips and lists of things to consider when getting ready to review, and in writing a fair and constructive review.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"Before accepting the invitation",nodeType:a}],nodeType:U},{data:{},content:[{data:{},marks:[],value:"When you receive an invitation to review, it is important to consider the following points before accepting.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[{type:p}],value:"Is the manuscript within my expertise?",nodeType:a},{data:{},marks:[],value:"\nThink about whether the manuscript is suitably within your area of expertise. If not, please decline the invite, and consider helping us by suggesting alternative relevant experts.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[{type:p}],value:"Do I have the time?",nodeType:a},{data:{},marks:[],value:"\nWe strive to keep our peer review process efficient and as such reviewers are requested to complete their reports within seven days after they accept the invite. You should let the editorial office know if you aren't able to provide a review but may be able to participate at another time.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[{type:p}],value:"Do I have a conflict of interest?",nodeType:a},{data:{},marks:[],value:"\nOnce the invitation is accepted, as a reviewer you will be asked to complete a short questionnaire regarding conflicts of interest to establish any relationship with the author(s) of the manuscript which may make it inappropriate for you to review. Conflicts of interest are assessed on a case by case basis and may not be disqualifying, so please disclose all answers in full. Further details on this are available ",nodeType:a},{data:{uri:"https:\u002F\u002Fwww.frontiersin.org\u002Fguidelines\u002Fpolicies-and-publication-ethics"},content:[{data:{},marks:[],value:Dy,nodeType:a}],nodeType:v},{data:{},marks:[],value:L,nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[{type:p}],value:"Respond to the invitation",nodeType:a},{data:{},marks:[],value:"\nWe ask reviewers to respond to the review invitation as soon as they can. You are of course free to decline to review if you feel that you lack the time or expertise, and we always appreciate recommendations for alternative reviewers.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"If a reviewer realizes that seven days will be insufficient to complete their review, or if there will be a delay to the deadline after the invitation has been accepted, they can contact the editorial office. We will be happy to assist.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"During peer review",nodeType:a}],nodeType:U},{data:{},content:[{data:{},marks:[],value:"Before a manuscript is sent for peer review, our research integrity team and the handling editor will already have carried out initial quality checks.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"The aims of our reviewers should always be to:",nodeType:a}],nodeType:f},{data:{},content:[{data:{},content:[{data:{},content:[{data:{},marks:[],value:"focus on the quality of the science objectively",nodeType:a}],nodeType:f}],nodeType:k},{data:{},content:[{data:{},content:[{data:{},marks:[],value:"collaborate towards improvement and think constructively",nodeType:a}],nodeType:f}],nodeType:k},{data:{},content:[{data:{},content:[{data:{},marks:[],value:"help the author and editor understand what is needed with clear comments.",nodeType:a}],nodeType:f}],nodeType:k}],nodeType:F},{data:{},content:[{data:{},marks:[],value:s,nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"Below are our tips for ensuring a quality report is produced.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"What to do",nodeType:a}],nodeType:fF},{data:{},content:[{data:{},marks:[{type:p}],value:"Respect the scope",nodeType:a},{data:{},marks:[],value:"\nAuthors provide a 'contribution to the field' statement with their manuscript to explain the article's intended scope and relevance. Keep the focus on what the manuscript is aiming to do, even if your expertise extends in a related direction. You should avoid recommending authors vastly expand the scope of the manuscript (e.g. \"you only dealt with x, you need to deal with y\"), or taking them outside their manuscript's intended scope.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[{type:p}],value:"Focus on science",nodeType:a},{data:{},marks:[],value:"\nBe objective – Frontiers discloses author names to reviewers for full transparency, however, reviewers are not asked to assess the author, only the manuscript. Also, it is not necessary to flag small copy-editing errors: our production team will ensure those are fixed during typesetting. Our processes ensure you should focus solely on the research itself.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[{type:p}],value:"Provide constructive feedback",nodeType:a},{data:{},marks:[],value:"\nComments should seek to recommend reasonable improvements, in a polite and impersonal tone. Show professional courtesy by thinking about what you would want to receive on a paper of your own. And if it is good, say so – and also say why!",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[{type:p}],value:"Consider field specifics",nodeType:a},{data:{},marks:[],value:"\nAre there elements of the research specific to the field you work in? If so, apply your expertise to give feedback on these. It will be helpful to all involved in the manuscript's review process.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"What not to do",nodeType:a}],nodeType:fF},{data:{},content:[{data:{},marks:[{type:p}],value:"Don't be vague or too brief",nodeType:a},{data:{},marks:[],value:"\nAuthors find precise and detailed feedback extremely helpful, and this tends to result in a timely and smoother review process. Whereas a brief report will often lead to additional questions from authors. Make sure recommendations and decisions are explained clearly. You should make good use of the detailed questionnaire provided in the collaborative review platform to provide a clear assessment.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[{type:p}],value:"Don't leave out key points in your initial report",nodeType:a},{data:{},marks:[],value:"\nThe initial report should be thorough and provide all the necessary feedback upfront. While it is possible that further revisions to the paper will bring up new questions, be sure to include your key points in your initial report. Ensure you conclude your report with a clear recommendation for the handling editor. You are the expert and your guidance is highly valued.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[{type:p}],value:"Don't drop out of the peer review",nodeType:a},{data:{},marks:[],value:"\nTo ensure an efficient process for all involved, please try to submit your responses on time. If you need to request an extension or to withdraw from the review process you can do this directly in the review forum at any time, or contact the editorial office for support. Try to place yourself in the authors' shoes, as they anxiously await feedback on their submission.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"Keep in touch",nodeType:a}],nodeType:fF},{data:{},content:[{data:{},marks:[],value:"Encountering any issues during review or have any concerns with the manuscript? Need assistance using our review platform? Need to request an extension to submit your review? For these or any other inquiries or updates, do not hesitate to ",nodeType:a},{data:{uri:xb},content:[{data:{},marks:[],value:"contact your journal's office",nodeType:a}],nodeType:v},{data:{},marks:[],value:L,nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"After peer review",nodeType:a}],nodeType:U},{data:{},content:[{data:{},marks:[],value:"Recognizing reviewers",nodeType:a}],nodeType:fF},{data:{},content:[{data:{},marks:[],value:"Reviewers who endorse a manuscript for publication are recognized for their work by being named on the published article. The names are listed alongside the editor's both on the published article page, and the article final files.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"But it is important to remember that if a reviewer recommends the rejection of a manuscript, or withdraws from the review process, their name will not be made available to the authors and is not published alongside the manuscript.",nodeType:a}],nodeType:f},{data:{},content:[{data:{},marks:[],value:"Irrespective of the reviewer's recommendation, when a report is submitted the reviewer will receive a confirmation email to acknowledge their work, including a copy of their report. While the report itself is not to be shared in any public forum, reviewers may use the confirmation email to obtain recognition for their involvement in the peer review process with their institution or other platforms. Their reports will also not be lost should they recommend rejection or withdraw from the review process later on.",nodeType:a}],nodeType:f}],nodeType:D}},links:{type:c,generated:e,id:"$ROOT_QUERY.basicContentPage({\"id\":\"3FJUcPYns4wtso1JmYkXvH\",\"preview\":false}).content.links",typename:ex},__typename:ew}},"$ROOT_QUERY.basicContentPage({\"id\":\"3FJUcPYns4wtso1JmYkXvH\",\"preview\":false}).content.links.assets":{writable:true,enumerable:true,value:{__typename:ey,block:[]}},"$ROOT_QUERY.basicContentPage({\"id\":\"3FJUcPYns4wtso1JmYkXvH\",\"preview\":false}).content.links":{writable:true,enumerable:true,value:{assets:{type:c,generated:e,id:"$ROOT_QUERY.basicContentPage({\"id\":\"3FJUcPYns4wtso1JmYkXvH\",\"preview\":false}).content.links.assets",typename:ey},entries:{type:c,generated:e,id:"$ROOT_QUERY.basicContentPage({\"id\":\"3FJUcPYns4wtso1JmYkXvH\",\"preview\":false}).content.links.entries",typename:ez},__typename:ex}},"$ROOT_QUERY.basicContentPage({\"id\":\"3FJUcPYns4wtso1JmYkXvH\",\"preview\":false}).content.links.entries":{writable:true,enumerable:true,value:{inline:[],block:[],__typename:ez}},"$ROOT_QUERY.institutionalMemberCollection({\"limit\":1,\"where\":{\"slug\":\"bips-institut\"}})":{writable:true,enumerable:true,value:{total:af,items:[{type:c,generated:e,id:"$ROOT_QUERY.institutionalMemberCollection({\"limit\":1,\"where\":{\"slug\":\"bips-institut\"}}).items.0",typename:ah}],__typename:ag}},"$ROOT_QUERY.institutionalMemberCollection({\"limit\":1,\"where\":{\"slug\":\"bips-institut\"}}).items.0":{writable:true,enumerable:true,value:{name:ajC,slug:"bips-institut",logo:{type:g,json:[{id:"EF1D959C-9D89-490A-B2D9BE478E946515",src:ajD,name:"Logo_ L